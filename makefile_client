all: utest_client

utest_client: test1_client.o clientSock.o
	g++ clientSock.o test1_client.o -o utest_client -pthread

test1_client.o: test1_server.cpp
	g++ -c test1_client.cpp -o test1_client.o

clientSock.o: clientSock.cpp
	g++ -c clientSock.cpp -o clientSock.o

clean:
	rm -rf *.o utest *.gcno *.gcda *.gcov